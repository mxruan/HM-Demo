.weui-cells_after-title {
  padding: 3px;
  display: flex;
}

.phone {
  height: 70rpx;
  float: left;
  width: 82%;
  font-size: 30rpx;
  background: #f2f2f2;
  border-radius: 8px;
  padding-left: 10px;
}

.search {
  height: 70rpx;
  font-size: 30rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
  width: 18%;
  border-radius: 5px;
  align-self: center;
}

.items {
  display: flex;
  justify-content: space-between;
  padding-left: 8px;
  padding-top: 8px;
  padding-right: 8px;
  padding-bottom: 8px;
  margin-left: 8px;
  margin-right: 8px;
  margin-top: 8px;
  background-color: #fff;
  border: 1px solid #eaeaea;
  border-bottom: 1px solid #d0d0d0;
  border-radius: 5px;
  padding: 5px;
  flex-direction: column;
  position: relative;
}

.item_count {
  font-size: 35rpx;
  color: #909090;
}

.item_service_name {
  font-size: 40rpx;
  color: black;
}

.user_name {
  margin-left: 25rpx;
  font-size: 30rpx;
  color: #ee5e7b;
}

.dividing {
  margin-top: 15rpx;
  margin-bottom: 5rpx;
  background-color: gainsboro;
  height: 1rpx;
}

.item {
  margin-left: 20rpx;
}

.service_type {
  font-size: 40rpx;
}

.service_name {
  margin-top: 5rpx;
  color: #6e6e6e;
  font-size: 35rpx;
}

.date_time {
  margin-top: 5rpx;
  color: #8e8e8e;
  font-size: 28rpx;
}

.more {
  position: absolute;
  width: 60rpx;
  height: 15rpx;
  right: 0rpx;
  bottom: 0rpx;
  src: '../../../image/icon_more';
}
